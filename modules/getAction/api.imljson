{
    "url": "/v2/{{ last(split(parameters.action_type, ':')) }}/{{ parameters.id }}",
    "method": "GET",
    "response": {
        "output": {
            "id": "{{ first(reverse(split(body._links.self.href, '/'))) }}",
            "creator_id": "{{ first(reverse(split(body._links['osdi:creator'].href, '/'))) }}",
            "action": "{{ body }}"
        }
    }
}