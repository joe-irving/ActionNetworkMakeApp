{
    "url": "/v2/tags",
    "method": "GET",
    "response": {
        "output": {
            "id": "{{ first(reverse(split(item._links.self.href, '/'))) }}",
            "tag": "{{ item }}"
        },
        "iterate": "{{body._embedded.`osdi:tags`}}",
        "limit": "{{ parameters.limit }}"
    }
}